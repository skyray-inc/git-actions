{
  "name": "gitactions",
  "version": "1.0.0",
  "description": "LogicHub git actions",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.js --license licenses.txt",
    "publish": "npm run build && git commit -am \"$MESSAGE\" --allow-empty && git tag -a -m \"$MESSAGE\" $VERSION && git push --follow-tags",
    "fix:eslint": "eslint -c config/.eslintrc --ignore-path config/.eslintignore ./ --fix",
    "fix:prettier": "prettier --config config/.prettierrc --ignore-path config/.prettierignore --write \"./**/*.{js,jsx,json,html,scss}\"",
    "lint": "npm run fix:prettier && npm run fix:eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyray-inc/git-actions.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skyray-inc/git-actions/issues"
  },
  "homepage": "https://github.com/skyray-inc/git-actions#readme",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/github": "5.0.1",
    "@octokit/graphql": "4.8.0"
  },
  "devDependencies": {
    "eslint": "8.13.0",
    "prettier": "2.6.2",
    "@vercel/ncc": "0.33.4"
  }
}
